<div class="container-fluid">
<p id="notice"><%= notice %></p>
  <br><br><br>
<center><h1>User Profile</h1></center><br>
<div class="container-fluid">
  <div class="row">
  <div class="offset-1 col-md-10 offset-1">
   <p id="notice"><%= notice %></p>
    <table id='home-table' class="table table-hover">
    <thead class="thead-light">
    <tr>
      <th>Name</th>
      <th>Gender</th>
      <th>Phone</th>
      <th>Image</th>
      <th>Password</th>
       <th>Confirmation</th>
      <th>Password Confirmation</th>

      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @homes.each do |home| %>
      <tr>
        <td><%= home.name %></td>
        <td><%= home.gender %></td>
        <td><%= home.phone %></td>
        <td><%= home.image %></td>
        <td><%= home.password %></td>
        <td><%= home.confirmation %></td>
        <td><%= home.password_confirmation %></td>
      </tr>
    <% end %>
  </tbody>
</table>