<p id="notice"><%= notice %></p>

<h1>Bilingaddresses</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Gender</th>
      <th>Phone</th>
      <th>Email</th>
      <th>Addressline1</th>
      <th>Addressline2</th>
      <th>Addressline3</th>
      <th>City</th>
      <th>State</th>
      <th>Country</th>
      <th>Zipcode</th>
      <th>Order</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bilingaddresses.each do |bilingaddress| %>
      <tr>
        <td><%= bilingaddress.name %></td>
        <td><%= bilingaddress.gender %></td>
        <td><%= bilingaddress.phone %></td>
        <td><%= bilingaddress.email %></td>
        <td><%= bilingaddress.addressline1 %></td>
        <td><%= bilingaddress.addressline2 %></td>
        <td><%= bilingaddress.addressline3 %></td>
        <td><%= bilingaddress.city %></td>
        <td><%= bilingaddress.state %></td>
        <td><%= bilingaddress.country %></td>
        <td><%= bilingaddress.zipcode %></td>
        <td><%= bilingaddress.order_id %></td>
        <td><%= bilingaddress.user_id %></td>
        <td><%= link_to 'Show', bilingaddress %></td>
        <td><%= link_to 'Edit', edit_bilingaddress_path(bilingaddress) %></td>
        <td><%= link_to 'Destroy', bilingaddress, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bilingaddress', new_bilingaddress_path %>
